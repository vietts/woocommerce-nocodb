# WooCommerce Configuration
# Get your credentials from: WordPress Admin → WooCommerce → Settings → Advanced → REST API
WC_STORE_URL=https://your-store.com
WC_CONSUMER_KEY=ck_your_consumer_key_here
WC_CONSUMER_SECRET=cs_your_consumer_secret_here

# NocoDB Configuration
# Get your token from: NocoDB → Account → Tokens
NOCODB_API_URL=https://app.nocodb.com/api/v2
NOCODB_API_TOKEN=your_nocodb_api_token_here
NOCODB_TABLE_CLIENTI=your_clienti_table_id_here
NOCODB_TABLE_ORDINI=your_ordini_table_id_here
